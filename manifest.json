{
    "manifest_version": 3,
    "name": "Time_Tracker",
    "description": "Track your time spent on each website",
    "version": "1.0",
    "icons": {
      "16": "icons/time-12-16.png",
      "32": "icons/time-12-32.png",
      "48": "icons/time-12-48.png",
      "128": "icons/time-12-128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/time-12-16.png",
        "32": "icons/time-12-32.png",
        "48": "icons/time-12-48.png",
        "128": "icons/time-12-128.png"
      }
    },
    "options_ui": {
      "page": "options.html"
      },
     
        
    "permissions": ["activeTab", "declarativeContent", "storage", "tabs"],
    "background": {
      "service_worker": "background.js"
    }
  }
  